{
  "buildCommand": "npm run build:tts",
  "outputDirectory": ".vitepress/dist",
  "rewrites": [
    {
      "source": "/(.+)",
      "destination": "/$1.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ]
}